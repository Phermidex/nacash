.slideshow-wrapper {
  .slideshow {
    position: relative;
    .slides {
      list-style: none;
      margin-bottom: 0;
      .slideshow__slide {
        display: none;
        position: relative;
        overflow: hidden;
        &.horizontal-center {
          .slideshow__text-wrap {
            .slideshow_wapper {
              align-items: center;
            }
          }
        }
        &.horizontal-left {
          .slideshow__text-wrap {
            .slideshow_wapper {
              align-items: flex-start;
            }
          }
        }
        &.horizontal-right {
          .slideshow__text-wrap {
            .slideshow_wapper {
              align-items: flex-end;
            }
          }
        }
        &.vertical-middle {
          .slideshow__text-wrap {
            .slideshow_wapper {
              justify-content: center;
            }
          }
        }
        &.vertical-top {
          .slideshow__text-wrap {
            .slideshow_wapper {
              justify-content: flex-start;
            }
          }
        }
        &.vertical-bottom {
          .slideshow__text-wrap {
            .slideshow_wapper {
              justify-content: flex-end;
            }
          }
        }
        .slideshow__text-wrap {
          top: 20px;
          bottom: 20px;
          left: 20px;
          right: 20px;
          position: absolute;
          z-index: 2;
          .slideshow_wapper {
            position: absolute;
            display: flex;
            flex-direction: column;
            width: 100%;
            height: 100%;
            .slideshow__text-content {
              .slide-text {
                &.slide-text-1 {
                  border: 0;
                  margin: 0;
                  padding: 0;
                  font-size: 36px;
                  line-height: 70px;
                  text-transform: capitalize;
                  letter-spacing: 0;
                  font-weight: 400;
                  white-space: nowrap;
                  @media (max-width: 767px) {
                    line-height: 36px;
                    font-size: 26px;
                  }
                }
                &.slide-text-2 {
                  line-height: 70px;
                  letter-spacing: 0;
                  font-weight: 500;
                  font-size: 70px;
                  white-space: nowrap;
                  @media (max-width: 767px) {
                    line-height: 40px;
                    font-size: 30px;
                  }
                }
                &.slide-text-3 {
                  letter-spacing: 0;
                  font-size: 16px;
                  line-height: 1.1;
                  margin-top: 18px;
                  white-space: nowrap;
                }
              }
              .button-slider-wr {
                .sl-button {
                  display: inline-block;
                  text-align: center;
                  font-size: 14px;
                  text-transform: uppercase;
                  margin-top: 50px;
                  letter-spacing: 1px;
                  font-weight: 700;
                  position: relative;
                  line-height: 1;
                  white-space: nowrap;
                  border: 0;
                  @media (max-width: 767px) {
                    margin-top: 10px;
                  }
                  &.sl-button-2 {
                    margin-left: 30px;
                  }
                }
                &.color_button_type_3 {
                  .sl-button {
                    border: 0;
                    @extend .clv-btn-3;
                    &:before {
                      content: "";
                      width: 100%;
                      position: absolute;
                      left: 0;
                      height: 2px;
                      bottom: -10px;
                      transition: all 0.3s;
                      background: $secon-bt-border_3;
                    }
                    &:hover {
                      &:before {
                        width: 0;
                        left: 0;
                      }
                    }
                  }
                }
                &.color_button_type_1 {
                  .sl-button {
                    font-size: 14px;
                    height: 40px;
                    line-height: 40px;
                    text-transform: uppercase;
                    font-weight: 500;
                    display: inline-block;
                    border-width: 1px;
                    border-style: solid;
                    padding: 0px 15px;
                    @extend .clv-btn-1;
                  }
                }
                &.color_button_type_2 {
                  .sl-button {
                    font-size: 14px;
                    height: 40px;
                    line-height: 40px;
                    text-transform: uppercase;
                    font-weight: 500;
                    display: inline-block;
                    border-width: 1px;
                    border-style: solid;
                    padding: 0px 15px;
                    @extend .clv-btn-2;
                  }
                }
              }
              .style-color-white {
                .slide-text {
                  &.slide-text-1 {
                    color: $white-slider-headding-1;
                  }
                  &.slide-text-2 {
                    color: $white-slider-headding-2;
                  }
                  &.slide-text-3 {
                    color: $white-slider-headding-3;
                  }
                }
              }
              .style-color-dark {
                .slide-text {
                  &.slide-text-1 {
                    color: $dark-slider-headding-1;
                  }
                  &.slide-text-2 {
                    color: $dark-slider-headding-2;
                  }
                  &.slide-text-3 {
                    color: $dark-slider-headding-3;
                  }
                }
              }
            }
          }
        }
      }
    }
    .flex-control-nav {
      list-style: none;
      position: absolute;
      bottom: 35px;
      margin-bottom: 0;
      text-align: center;
      z-index: 9;
      left: 50%;
      transform: translateX(-50%);
      -webkit-transform: translateX(-50%);
      -moz-transform: translateX(-50%);
      -ms-transform: translateX(-50%);
      line-height: 0;
      padding: 5px;
      background: rgba(0, 0, 0, 0.2);
      border-radius: 20px;
      width: auto;
      li {
        position: relative;
        display: inline-block;
        width: auto;
        height: auto;
        margin: 0 5px;
        padding: 0;
        cursor: pointer;
        a {
          display: block;
          cursor: pointer;
          text-indent: -9999px;
          position: relative;
          overflow: hidden;
          border-radius: 50%;
          width: 15px;
          height: 15px;
          -webkit-transition: background 0.3s ease;
          transition: background 0.3s ease;
          font-size: 0;
          box-shadow: inset 0 0 0 2px #fff;
          &.flex-active {
            background: #fff;
          }
        }
      }
    }
    .flex-direction-nav {
      list-style: none;
      top: 50%;
      -webkit-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
      transform: translateY(-50%);
      position: absolute;
      width: 100%;
      z-index: 9;
      li {
        a {
          top: 50%;
          -webkit-transform: translate(0,-50%);
          -ms-transform: translate(0,-50%);
          transform: translate(0,-50%);
          line-height: 60px;
          font-size: 54px;
          position: absolute;
          z-index: 9;
          color: #fff;
          &.flex-prev {
            left: 50px;
          }
          &.flex-next {
            right: 50px;
          }
        }
      }
    }
  }
  &.button_size_medium {
    .slideshow .flex-direction-nav li a {
      font-size: 27px;
    }
  }
  &.button_size_small {
    .slideshow .flex-direction-nav li a {
      font-size: 18px;
    }
  }
}
