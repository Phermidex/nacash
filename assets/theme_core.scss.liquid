.template-product {
  .contact-forms {
    background-color: rgba(128, 128, 128, 0.3);
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    z-index: 99999;
    right: 0;
    left: 0;
    bottom: 0;
    text-align: center;
    .contact-form-contents {
      width: 506px;
      vertical-align: middle;
      margin: 30px auto;
      display: inline-block;
      position: relative;
      .contact-form-body {
        background: #fff;
        padding: 40px;
        input, textarea {
          background: #fff;
          font-size: 14px;
          border: 1px solid #000;
          padding: 11px 12px 10px 15px;
          box-shadow: none;
          outline: none;
          color: #000;
          margin-bottom: 20px;
          width: 100%;
        }
        textarea {
          height: 160px;
        }
        input.btn {
          width: auto;
          display: block;
          border: none;
          background: #000;
          color: #fff;
          border: 1px solid #000;
          font-size: 14px;
          line-height: 1;
          font-weight: 400;
          letter-spacing: 0.5px;
          position: relative;
          outline: none;
          padding: 6px 31px 4px;
          text-align: center;
          height: 40px;
          cursor: pointer;
          margin-bottom: 0;
          &:hover {
            opacity: 0.6;
          }
        }
        #contactFormWrapper {
          h3 {
            color: #191919;
            font-size: 16px;
            font-weight: 500;
            padding-bottom: 5px;
            letter-spacing: 0.5px;
            text-align: left;
            margin-bottom: 15px;
          }
          span {
            color: #191919;
            font-size: 16px;
            font-weight: 500;
            padding-bottom: 5px;
            letter-spacing: 0.5px;
            text-align: left;
            margin-bottom: 15px;
            float:left;
            width:100%;
          }
        }
      }
      .ask-product-close {
        position: relative;
        padding: 20px 20px 0px 20px;
        width: 100%;
        height: 50px;
        display: inline-block;
        i {
          width: 30px;
          height: 30px;
          top: 40px;
          line-height: 30px;
          position: absolute;
          text-align: center;
          color: #ffffff;
          z-index: 99999;
          background: #000;
          right: -15px;
          font-size: 14px;
          &:hover {
            color: #fff;
            background: {{ settings.body_link_color_hover }};
          }
        }
      }
    }
    &:before {
      content: '';
      display: inline-block;
      height: 100%;
      vertical-align: middle;
      margin-right: -4px;
    }
  }
  &.open-ask-product {
    .contact-forms {
      display: block !important;
      overflow-x: hidden;
      overflow-y: auto;
    }
  }
  .delivery-popup {
    text-align: center;
    position: fixed;
    top: 50%;
    left: 50%;
    z-index: 999999;
    width: 100%;
    transform: translate(-50%, -50%);
    .delivery-popup-contents {
      max-width: 962px;
      margin: 30px auto;
      text-align: left;
      .delivery-content {
        padding: 35px 40px 30px 35px;
        background: #ffffff;
        span {
          color: #191919;
          font-size: 16px;
          font-weight: 500;
          padding-bottom: 5px;
          letter-spacing: 0.5px;
          margin-bottom:5px;
          width:100%;
          float:left;
        }
        li {
          font-size: 14px;
          position: relative;
          padding-left: 10px;
        }
      }
    }
    .delivery-product-close {
      position: relative;
      padding: 20px 20px 0px 20px;
      width: 100%;
      height: 50px;
      display: inline-block;
      i {
        width: 30px;
        height: 30px;
        top: 40px;
        line-height: 30px;
        position: absolute;
        text-align: center;
        color: #ffffff;
        z-index: 99999;
        background: #000;
        right: -15px;
        font-size: 14px;
        &:hover {
          color: #fff;
          background: {{ settings.body_link_color_hover }};
        }
      }
    }
  }
  &.open-delivery-product {
    .delivery-popup {
      display: block !important;
    }
  }
  .product-label-wr {
    > div {
      display: inline-block;
      position: static;
      box-shadow: none;
      border: 0;
      padding: 0;
      color: #fff;
      text-transform: capitalize;
    }
    .product-label,.label-new {
      width: 45px;
      height: 23px;
      line-height: 23px;
      font-size: 11px;
      background: #ff525d;
    }
    .label-new {
      background: #00afec;
    }
    .availability {
      .product-inventory {
        margin-bottom: 0;
        label{
          margin-right:10px;
          display: none;
        }
        span {
          display: inline-block;
          font-size: 11px;
          line-height: 23px;
          font-weight: 400;
          background: #81bf30;
          letter-spacing: 0;
          height: 23px;
          padding: 0 5px;
          text-transform: capitalize;
        }
      }
    }
  }
}
