      &:hover {
        &:after { 
          -webkit-transform: rotate(45deg);
          -ms-transform: rotate(45deg);
          transform: rotate(45deg);
          content: "";
          width: 5px;
          height: 8px;
          display: block;
          border: solid #252525;
          border-width: 0 2px 2px 0;
          position: absolute;
          top: 6px;
          left: 5px;
          opacity: 1;
          z-index: 9;
        }
      }
    }
    &.active {
      a {
        &:after { 
          -webkit-transform: rotate(45deg);
          -ms-transform: rotate(45deg);
          transform: rotate(45deg);
          content: "";
          width: 5px;
          height: 8px;
          display: block;
          border: solid #252525;
          border-width: 0 2px 2px 0;
          position: absolute;
          top: 6px;
          left: 5px;
          opacity: 1;
          z-index: 9;
